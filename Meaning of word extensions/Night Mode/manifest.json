{
    "name":"Night Mode",
    "description":"enables night mode",
    "version":"1.0.0",
    "manifest_version":2,
    "background":{
        "scripts":[
            "popup.js"
        ],
        "persistent":true
    },
    "browser_action":{
        "default_title":"Night Mode",
        "default_popup":"popup.html"
    },
    "permissions":[
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}